all:
	$(MAKE) -C menu
	$(MAKE) -C pacman
	$(MAKE) -C nibbler

clean:
	$(MAKE) -C menu clean
	$(MAKE) -C pacman clean
	$(MAKE) -C nibbler clean

fclean:	clean
	$(MAKE) -C menu fclean
	$(MAKE) -C pacman fclean
	$(MAKE) -C nibbler fclean